%h1 New Todo
= form_for @todo do |f|
  = f.label      :title
  = f.text_field :title
  = f.label      :description
  = f.text_field :description
  = f.label       :date_created
  = date_field    :date_created, Date.today
  = f.label       :date_due
  = date_field    :date_due, Date.today
  = f.label       :is_complete
  = f.check_box   :is_complete
  = f.submit    